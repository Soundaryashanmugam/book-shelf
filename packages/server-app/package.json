  "scripts": {
    "tsc": "tsc",
    "tsc-watch": "tsc -w",
    "build": "gulp build",
    "start": "node build/src/index.js",
    "test": "gulp test",
    "watch": "nodemon --watch build/src build/src/index.js",
    "gulp": "gulp build && gulp watch",
